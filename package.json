{
    "name": "@nmsmith/eslint-config",
    "description": "The eslint standard for nmsmith22389.",
    "version": "1.0.0",
    "main": "dist/index.js",
    "license": "MIT",
    "author": "nmsmith89 <nmsmith22389@gmail.com>",
    "homepage": "https://github.com/nmsmith22389/eslint-config#readme",
    "bugs": {
        "url": "https://github.com/nmsmith22389/eslint-config/issues"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:nmsmith22389/eslint-config.git"
    },
    "files": [
        "dist/**/*",
        "types"
    ],
    "scripts": {
        "prepare": "npm run clean; npm run build",
        "build-old": "ts-node-script scripts/build.ts",
        "build": "tsc -b tsconfig.build.json",
        "clean": "rm -r ./dist",
        "format": "prettier --write \"./**/*.{ts,js,json,md}\" --ignore-path ../../.prettierignore",
        "get-config": "ts-node-script scripts/get-config.ts",
        "lint": "eslint . --ext .js,.ts",
        "typecheck": "tsc -p tsconfig.json --noEmit"
    },
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "@typescript-eslint/parser": "^2.31.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-import": "^2.20.2",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-vue": "^6.2.2",
        "prettier": "^2.0.5"
    },
    "devDependencies": {
        "@types/estree": "0.0.44",
        "@types/node": "^13.13.5",
        "chalk": "^4.0.0",
        "cross-env": "^7.0.2",
        "eslint": "^6.8.0",
        "json-schema": "^0.2.5",
        "ts-node": "^8.10.1",
        "typescript": "^3.8.3",
        "vscode": "^1.1.37"
    },
    "peerDependencies": {
        "eslint": "^6.8.0"
    }
}
